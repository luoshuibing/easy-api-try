{
  "name": "easy-api-try",
  "displayName": "Easy API Try",
  "description": "在面板中调试接口，支持扫描Spring Boot项目URL",
  "version": "0.1.5",
  "publisher": "luoshuibing",
  "icon": "media/icon.png",
  "license": "MIT",
  "keywords": [
    "api",
    "spring-boot",
    "debugger",
    "webview",
    "trae",
    "http",
    "json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/luoshuibing/easy-api-try"
  },
  "bugs": {
    "url": "https://github.com/luoshuibing/easy-api-try/issues"
  },
  "homepage": "https://github.com/luoshuibing",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "trae.scanSpringBootEndpoints",
        "title": "扫描Spring Boot接口并打开调试器"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -w -p .",
    "package:openvsx": "npx --yes ovsx package",
    "publish:openvsx": "powershell -NoProfile -ExecutionPolicy Bypass -File ./publish-openvsx.ps1"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "@types/node": "^20.11.30",
    "@types/vscode": "^1.80.0"
  }
}
